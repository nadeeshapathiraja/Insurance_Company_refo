                                                <div class="input-group date datepicker" data-date="{{policyRenewalHistory.RenewalDate|date:'MM/dd/yyyy' }}"
                                                     data-date-format="mm/dd/yyyy" ng-model="policyRenewalHistory.RenewalDate">
                                                    <input type="text" ng-style="{'background-color': data.backgroundCol}"
                                                           id="dp-3" class="form-control  " value="{{policyRenewalHistory.RenewalDate|date:'MM/dd/yyyy' }}" />
                                                    <span class="input-group-addon add-on">
                                                        <span class="glyphicon glyphicon-calendar"></span>
                                                    </span>
                                                </div>

                                            </div>





                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-4 control-label">Notification Date</label>
                                            <div class="col-md-4">
                                                <div class="input-group date datepicker" data-date="{{policyRenewalHistory.NotificationDate|date:'MM/dd/yyyy' }}"
                                                     data-date-format="mm/dd/yyyy" ng-model="policyRenewalHistory.NotificationDate">
