                                                           id="dp-4" class="form-control  " value="{{policyRenewalHistory.NotificationDate|date:'MM/dd/yyyy'}}" />
                                                    <span class="input-group-addon add-on">
                                                        <span class="glyphicon glyphicon-calendar"></span>
                                                    </span>
                                                </div>

                                            </div>




                                        </div>


                                        <div class="form-group">


                                            <label class="col-md-4 control-label">Cancel Policy</label>

                                            <div class="col-md-8">

                                                <input type="checkbox" 
                                                       ng-model="policyRenewalHistory.IsCancel" />
                                            </div>

                                        </div>
                                        <div class="form-group">


                                            <label class="col-md-4 control-label">notyfy Insurance Company</label>

                                            <div class="col-md-8">

                                                <input type="checkbox"   ng-model="policyRenewalHistory.IsSent"  />
                                            </div>
                                         
                                        </div>

                                     
                                        <button class="btn btn-primary pull-right" ng-click="RenewPolicy()">Renew Policy <span class="fa fa-floppy-o fa-right"></span></button>









