                                                        <textarea ui-tinymce="tinymceOptions" ng-model="emlObj.Messege">
                                                        </textarea>
                                                    </div>

                                                </div>
                                                <div class="form-group">
                                                    <div class="col-md-12">
                                                        
                                                        <div class="pull-right" hidden>
                                                            <button class="btn btn-danger" ng-click="saveEmail()"><span class="fa fa-envelope"></span> Send Message</button>
                                                           </div>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>

                                    </div>
                                    <!-- END CONTENT FRAME BODY -->
                                </div>
                                <!-- END CONTENT FRAME -->
                            </div>
                        </div>

